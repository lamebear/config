alias h="cd ~"
alias c="clear"
alias cwd="echo $PWD"
alias ll="ls -al"
alias edit_profile="vi ~/.bash_profile"
alias edit_alias="vi ~/.bash_aliases && source ~/.bash_aliases"
alias edit_funcs="vi ~/.bash_functions && source ~/.bash_functions"
alias reload_profile=". ~/.bash_profile"
alias weave-lab="code ~/go/src/github.com/weave-lab/"
alias schedx="code ~/go/src/bitbucket.org/schedx/"
alias omgrpc="nw ~/code/node/omgrpc/"
alias utc="date +%s"

alias prodweave='cp ~/.weave/prod-config.ini ~/Library/Application\ Support/Weave/config.ini'
alias devweave='cp ~/.weave/dev-config.ini ~/Library/Application\ Support/Weave/config.ini'
